{
  "expo": {
    "name": "ListaCerta",
    "slug": "listacerta-mobile",
    "scheme": "listacerta",
    "version": "0.1.0",
    "orientation": "portrait",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router",
      "expo-camera",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.listacerta.mobile"
    },
    "android": {
      "package": "com.listacerta.mobile"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
