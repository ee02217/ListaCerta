{
  "name": "@listacerta/mobile",
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "dev": "EXPO_ROUTER_APP_ROOT=app expo start --dev-client",
    "dev:client": "EXPO_ROUTER_APP_ROOT=app expo start --dev-client",
    "ios": "EXPO_ROUTER_APP_ROOT=app expo run:ios",
    "android": "EXPO_ROUTER_APP_ROOT=app expo run:android",
    "web": "EXPO_ROUTER_APP_ROOT=app expo start --web",
    "typecheck": "tsc --noEmit",
    "build": "echo \"Mobile build is handled via EAS/Expo\"",
    "clean": "rm -rf .expo dist"
  },
  "dependencies": {
    "@listacerta/shared-types": "*",
    "expo": "~52.0.0",
    "expo-camera": "~16.0.18",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-dev-menu-interface": "^1.9.3",
    "expo-linking": "~7.0.5",
    "expo-manifests": "^0.15.8",
    "expo-router": "~4.0.22",
    "expo-sqlite": "~15.1.4",
    "expo-status-bar": "~2.0.0",
    "expo-updates-interface": "~1.0.0",
    "react": "18.3.1",
    "react-native": "0.76.3",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/react": "~18.3.12",
    "typescript": "~5.3.3"
  }
}
